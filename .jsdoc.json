{
    "source": {
        "include": "./lib",
        "includePattern": ".+\\.js(doc|x)?",
        "excludePattern": ".+\\test.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "template": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destinations": "./docs/"
    }
}